{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "error",
    "quotes": "off",
    "indent": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off"
  }
}
